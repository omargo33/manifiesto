<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">    <af:panelHeader text="#{BaseBean.nameUser} no tiene asignado ninguna aerolinea" id="ph1"                    rendered="#{sessionScope.idAerolinea eq 0}" size="0" headerLevel="1" messageType="warning">        <f:facet name="context"/>        <f:facet name="toolbar"/>        <f:facet name="menuBar"/>        <f:facet name="legend"/>        <f:facet name="info"/>    </af:panelHeader>    <af:panelHeader text="Aerolinea asignado: #{sessionScope.aerolineaDescripcion}" id="ph2"                    rendered="#{sessionScope.idAerolinea ne 0}" size="0" headerLevel="1" messageType="confirmation">        <f:facet name="context"/>        <f:facet name="toolbar"/>        <f:facet name="menuBar"/>        <f:facet name="legend"/>        <f:facet name="info"/>    </af:panelHeader></jsp:root>