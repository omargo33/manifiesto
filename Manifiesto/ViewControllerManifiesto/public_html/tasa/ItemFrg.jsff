<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"    xmlns:af="http://xmlns.oracle.com/adf/faces/rich"    xmlns:f="http://java.sun.com/jsf/core"    xmlns:bl01="/baseLib01">    <af:pageTemplate viewId="/plantilla/itemFrg.jspx" id="pt1">        <f:facet name="elementoCentro">            <af:panelFormLayout id="pfl1" labelWidth="200">                <af:inputText value="#{bindings.Nombre.inputValue}" label="#{bindings.Nombre.hints.label}"                              required="#{bindings.Nombre.hints.mandatory}"                              columns="#{bindings.Nombre.hints.displayWidth}"                              maximumLength="#{bindings.Nombre.hints.precision}"                              shortDesc="#{bindings.Nombre.hints.tooltip}" id="it2">                    <f:validator binding="#{bindings.Nombre.validator}"/>                </af:inputText>                <af:inputText value="#{bindings.Tasa1.inputValue}" label="#{bindings.Tasa1.hints.label}"                              required="#{bindings.Tasa1.hints.mandatory}"                              columns="#{bindings.Tasa1.hints.displayWidth}"                              maximumLength="#{bindings.Tasa1.hints.precision}"                              shortDesc="#{bindings.Tasa1.hints.tooltip}" id="it3">                    <f:validator binding="#{bindings.Tasa1.validator}"/>                    <af:convertNumber groupingUsed="false" pattern="#{bindings.Tasa1.format}"/>                </af:inputText>                <af:inputText value="#{bindings.Timbre.inputValue}" label="#{bindings.Timbre.hints.label}"                              required="#{bindings.Timbre.hints.mandatory}"                              columns="#{bindings.Timbre.hints.displayWidth}"                              maximumLength="#{bindings.Timbre.hints.precision}"                              shortDesc="#{bindings.Timbre.hints.tooltip}" id="it4">                    <f:validator binding="#{bindings.Timbre.validator}"/>                    <af:convertNumber groupingUsed="false" pattern="#{bindings.Timbre.format}"/>                </af:inputText>                <f:facet name="footer">                    <bl01:botonesGuardarBorrar id="bgb1" actualizarAccion="#{BackingItemFrg.actionGuardar}" actualizarPermiso="#{pageFlowScope.pPermisoActualizar}" borrarAccion="#{BackingItemFrg.actionDelete}" borrarDisabled="#{bindings.IdTasa.inputValue eq 0}" borrarPermiso="#{pageFlowScope.pPermisoBorrar}"/>                </f:facet>            </af:panelFormLayout>        </f:facet>        <f:facet name="elementoCancelar">            <bl01:botonesCancelar id="bc1" inicioAccion="Inicio">                <f:facet name="botonesAcciones"/>            </bl01:botonesCancelar>        </f:facet>        <f:facet name="elementoAuditoria">            <af:panelFormLayout id="pfl2" labelWidth="170">                <af:inputText value="#{bindings.Usuario.inputValue}" label="#{bindings.Usuario.hints.label}"                              required="#{bindings.Usuario.hints.mandatory}"                              columns="#{bindings.Usuario.hints.displayWidth}"                              maximumLength="#{bindings.Usuario.hints.precision}"                              shortDesc="#{bindings.Usuario.hints.tooltip}" id="it5">                    <f:validator binding="#{bindings.Usuario.validator}"/>                </af:inputText>                <af:inputDate value="#{bindings.UsuarioFecha.inputValue}" label="#{bindings.UsuarioFecha.hints.label}"                              required="#{bindings.UsuarioFecha.hints.mandatory}"                              columns="#{bindings.UsuarioFecha.hints.displayWidth}"                              shortDesc="#{bindings.UsuarioFecha.hints.tooltip}" id="id1">                    <f:validator binding="#{bindings.UsuarioFecha.validator}"/>                    <af:convertDateTime pattern="#{bindings.UsuarioFecha.format}"/>                </af:inputDate>                <af:inputText value="#{bindings.UsuarioPrograma.inputValue}"                              label="#{bindings.UsuarioPrograma.hints.label}"                              required="#{bindings.UsuarioPrograma.hints.mandatory}"                              columns="#{bindings.UsuarioPrograma.hints.displayWidth}"                              maximumLength="#{bindings.UsuarioPrograma.hints.precision}"                              shortDesc="#{bindings.UsuarioPrograma.hints.tooltip}" id="it6">                    <f:validator binding="#{bindings.UsuarioPrograma.validator}"/>                </af:inputText>                <af:inputText value="#{bindings.IdTasa.inputValue}" label="#{bindings.IdTasa.hints.label}"                              required="#{bindings.IdTasa.hints.mandatory}"                              columns="#{bindings.IdTasa.hints.displayWidth}"                              maximumLength="#{bindings.IdTasa.hints.precision}"                              shortDesc="#{bindings.IdTasa.hints.tooltip}" id="it1">                    <f:validator binding="#{bindings.IdTasa.validator}"/>                    <af:convertNumber groupingUsed="false" pattern="#{bindings.IdTasa.format}"/>                </af:inputText>            </af:panelFormLayout>        </f:facet>        <f:attribute name="permisoVerAuditoria" value="#{pageFlowScope.pPermisoVerAuditoria}"/>        <f:attribute name="titulo" value="#{bnd.tasa_itemFrg_titulo}"/>        <f:attribute name="tituloDescripcion" value="#{bnd.tasa_itemFrg_descripcion}"/>    </af:pageTemplate></jsp:root>