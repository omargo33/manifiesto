<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="info-usuario-aeropuerto-task-flow">    <default-activity>getIdAerolineaNick</default-activity>    <data-control-scope>      <shared/>    </data-control-scope>    <exception-handler>vacio</exception-handler>    <view id="aerolinea">      <page>/infoUsuarioAeropuerto/aerolinea.jsff</page>    </view>    <router id="router1">      <case id="__8">        <expression>#{sessionScope.idAerolinea == 0}</expression>        <outcome>noBusca</outcome>      </case>      <default-outcome>getIdAeropuertoNick</default-outcome>    </router>    <method-call id="getIdAerolineaNick">      <method>#{bindings.getIdAerolineaNick.execute}</method>      <return-value>#{sessionScope.idAerolinea}</return-value>      <outcome>        <fixed-outcome>getIdAerolineaNick</fixed-outcome>      </outcome>    </method-call>    <method-call id="ejecutarByIndiceReturn1">      <method>#{bindings.ejecutarByIndiceReturn.execute}</method>      <return-value>#{sessionScope.aerolineaDescripcion}</return-value>      <outcome>        <fixed-outcome>ejecutarByIndiceReturn</fixed-outcome>      </outcome>    </method-call>    <view id="vacio">      <page>/infoUsuarioAeropuerto/vacio.jsff</page>    </view>    <control-flow-rule id="__2">      <from-activity-id>router1</from-activity-id>      <control-flow-case id="__7">        <from-outcome>noBusca</from-outcome>        <to-activity-id>aerolinea</to-activity-id>      </control-flow-case>      <control-flow-case id="__9">        <from-outcome>getIdAeropuertoNick</from-outcome>        <to-activity-id>ejecutarByIndiceReturn1</to-activity-id>      </control-flow-case>    </control-flow-rule>    <control-flow-rule id="__6">      <from-activity-id>getIdAerolineaNick</from-activity-id>      <control-flow-case id="__10">        <from-outcome>getIdAerolineaNick</from-outcome>        <to-activity-id>router1</to-activity-id>      </control-flow-case>    </control-flow-rule>    <control-flow-rule id="__11">      <from-activity-id>ejecutarByIndiceReturn1</from-activity-id>      <control-flow-case id="__12">        <from-outcome>ejecutarByIndiceReturn</from-outcome>        <to-activity-id>aerolinea</to-activity-id>      </control-flow-case>    </control-flow-rule>    <use-page-fragments/>  </task-flow-definition></adfc-config>