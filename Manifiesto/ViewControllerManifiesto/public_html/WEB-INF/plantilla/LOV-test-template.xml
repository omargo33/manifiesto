<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-template id="LOV-test-template">        <default-activity>router1</default-activity>        <data-control-scope>      <shared/>    </data-control-scope>        <input-parameter-definition id="__3">            <name>idValueInt</name>            <value>#{pageFlowScope.idValueInt}</value>            <class>java.lang.Integer</class>            <required/>        </input-parameter-definition>                        <input-parameter-definition id="__1">      <name>buscar</name>      <value>#{pageFlowScope.buscar}</value>      <class>java.lang.Boolean</class>      <required/>    </input-parameter-definition>        <return-value-definition id="__4">            <name>returnValueCodeInt</name>            <value>#{pageFlowScope.returnValueCodeInt}</value>            <class>java.lang.Integer</class>        </return-value-definition>        <return-value-definition id="__6">            <name>returnValueCodeString</name>            <value>#{pageFlowScope.returnValueCodeString}</value>            <class>java.lang.String</class>        </return-value-definition>        <managed-bean id="__5">            <managed-bean-name>BackingLOVFrg</managed-bean-name>            <managed-bean-class>view.plantilla.LOVFrgBean</managed-bean-class>            <managed-bean-scope>session</managed-bean-scope>        </managed-bean>        <router id="router1">            <case id="__10">                <expression>#{pageFlowScope.idValueInt &gt; 0}</expression>                <outcome>buscar</outcome>            </case>            <default-outcome>noBuscar</default-outcome>        </router>        <view id="LOV">        </view>        <method-call id="buscarIndice">            <outcome>                <fixed-outcome>buscarIndice</fixed-outcome>            </outcome>        </method-call>        <control-flow-rule id="__100">            <from-activity-id>router1</from-activity-id>            <control-flow-case id="__2">                <from-outcome>noBuscar</from-outcome>                <to-activity-id>LOV</to-activity-id>            </control-flow-case>            <control-flow-case id="__7">                <from-outcome>buscar</from-outcome>                <to-activity-id>buscarIndice</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__8">            <from-activity-id>buscarIndice</from-activity-id>            <control-flow-case id="__9">                <from-outcome>buscarIndice</from-outcome>                <to-activity-id>LOV</to-activity-id>            </control-flow-case>        </control-flow-rule>        <use-page-fragments/>    </task-flow-template></adfc-config>