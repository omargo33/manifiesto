<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="error-rest-task-flow">    <template-reference>      <document>/WEB-INF/plantilla/maestro-template.xml</document>      <id>maestro-template</id>    </template-reference>    <managed-bean id="__1">      <managed-bean-name>PPRBackingErrorRest</managed-bean-name>      <managed-bean-class>viewManifiesto.errorRest.PPRListaFrg</managed-bean-class>      <managed-bean-scope>request</managed-bean-scope>    </managed-bean>    <view id="ListaFrg">      <page>/errorRest/ListaFrg.jsff</page>    </view>    <method-call id="Rollback">      <method>#{bindings.Rollback.execute}</method>      <outcome>        <fixed-outcome>Rollback</fixed-outcome>      </outcome>    </method-call>    <method-call id="ejecutarConsultaErrorJDE">      <method>#{bindings.ejecutarConsultaErrorJDE.execute}</method>      <outcome>        <fixed-outcome>ejecutarConsultaErrorJDE</fixed-outcome>      </outcome>    </method-call>    <control-flow-rule id="__2">      <from-activity-id>ejecutarConsultaErrorJDE</from-activity-id>      <control-flow-case id="__3">        <from-outcome>ejecutarConsultaErrorJDE</from-outcome>        <to-activity-id>ListaFrg</to-activity-id>      </control-flow-case>    </control-flow-rule>    <control-flow-rule id="__8">      <from-activity-id>router</from-activity-id>      <control-flow-case id="__9">        <from-outcome>Listar</from-outcome>        <to-activity-id>ejecutarConsultaErrorJDE</to-activity-id>      </control-flow-case>    </control-flow-rule>    <use-page-fragments/>  </task-flow-definition></adfc-config>