<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">    <task-flow-definition id="manifiesto-usuario-task-flow">        <template-reference>            <document>/WEB-INF/plantilla/maestro-template.xml</document>            <id>maestro-template</id>        </template-reference>        <managed-bean id="__20">            <managed-bean-name>PPRBackingManifiestoUsuarioItemFrg</managed-bean-name>            <managed-bean-class>viewManifiesto.manifiestoUsuario.PPRItemFrg</managed-bean-class>            <managed-bean-scope>request</managed-bean-scope>        </managed-bean>        <managed-bean id="__23">            <managed-bean-name>PPRBackingManifiestoUsuarioListaFrg</managed-bean-name>            <managed-bean-class>viewManifiesto.manifiestoUsuario.PPRListaFrg</managed-bean-class>            <managed-bean-scope>backingBean</managed-bean-scope>        </managed-bean>        <managed-bean id="__27">            <managed-bean-name>SesionBean</managed-bean-name>            <managed-bean-class>viewManifiesto.manifiestoUsuario.SessionBean</managed-bean-class>            <managed-bean-scope>session</managed-bean-scope>        </managed-bean>        <managed-bean id="__38">            <managed-bean-name>PPRBackingErrorRest</managed-bean-name>            <managed-bean-class>viewManifiesto.errorRest.PPRListaFrg</managed-bean-class>            <managed-bean-scope>request</managed-bean-scope>        </managed-bean>        <router id="router1">            <case id="__8">                <expression>#{sessionScope.idUsuario == 0}</expression>                <outcome>ko</outcome>            </case>            <case id="__11">                <expression>#{sessionScope.idUsuario != 0}</expression>                <outcome>ok</outcome>            </case>            <default-outcome>ok</default-outcome>        </router>        <view id="noConfigurado">            <page>/manifiestoUsuario/noConfigurado.jsff</page>        </view>        <view id="ItemFrg">            <page>/manifiestoUsuario/ItemFrg.jsff</page>        </view>        <view id="ListaFrg">            <page>/manifiestoUsuario/ListaFrg.jsff</page>        </view>        <method-call id="getIdUsuarioNick">            <method>#{bindings.getIdUsuarioNick.execute}</method>            <return-value>#{sessionScope.idUsuario}</return-value>            <outcome>                <fixed-outcome>getIdUsuarioNick</fixed-outcome>            </outcome>        </method-call>        <method-call id="Create">            <method>#{bindings.CreateWithParams.execute}</method>            <outcome>                <fixed-outcome>Create</fixed-outcome>            </outcome>        </method-call>        <method-call id="ejecutarByIndice">            <method>#{bindings.ejecutarByIndice.execute}</method>            <outcome>                <fixed-outcome>Editar</fixed-outcome>            </outcome>        </method-call>        <method-call id="getIdAerolineaNick">            <method>#{bindings.getIdAerolineaNick.execute}</method>            <return-value>#{sessionScope.idAerolinea}</return-value>            <outcome>                <fixed-outcome>getIdAerolineaNick</fixed-outcome>            </outcome>        </method-call>        <method-call id="getIdAeropuertoNick">            <method>#{bindings.getIdAeropuertoNick.execute}</method>            <return-value>#{sessionScope.idAeropuerto}</return-value>            <outcome>                <fixed-outcome>getIdAeropuertoNick</fixed-outcome>            </outcome>        </method-call>        <method-call id="ejecutarConsulta">            <method>#{bindings.ejecutarConsulta.execute}</method>            <outcome>                <fixed-outcome>ejecutarConsulta</fixed-outcome>            </outcome>        </method-call>        <method-call id="ejecutarByIndiceReturn_Aerolinea">            <method>#{bindings.ejecutarByIndiceReturn.execute}</method>            <return-value>#{sessionScope.aerolineaDescripcion}</return-value>            <outcome>                <fixed-outcome>ejecutarByIndiceReturn</fixed-outcome>            </outcome>        </method-call>        <method-call id="ejecutarByIndiceReturn_Aeropuerto">            <method>#{bindings.ejecutarByIndiceReturn.execute}</method>            <return-value>#{sessionScope.aeropuertoDescripcion}</return-value>            <outcome>                <fixed-outcome>ejecutarByIndiceReturn</fixed-outcome>            </outcome>        </method-call>        <method-call id="SessionBean_Limpar">            <method>#{SesionBean.limpiar}</method>            <outcome>                <fixed-outcome>limpiar</fixed-outcome>            </outcome>        </method-call>        <method-call id="base_isUsuarioRol">            <method>#{bindings.base_isUsuarioRol.execute}</method>            <return-value>#{sessionScope.isCLI02}</return-value>            <outcome>                <fixed-outcome>base_isUsuarioRol</fixed-outcome>            </outcome>        </method-call>        <router id="router2">            <case id="__37">                <expression>#{sessionScope.isCLI02}</expression>                <outcome>isCLI02</outcome>            </case>            <default-outcome>noIsCLI02</default-outcome>        </router>                                        <control-flow-rule id="__5">            <from-activity-id>router1</from-activity-id>            <control-flow-case id="__7">                <from-outcome>ko</from-outcome>                <to-activity-id>noConfigurado</to-activity-id>            </control-flow-case>            <control-flow-case id="__1">                <from-outcome>ok</from-outcome>                <to-activity-id>getIdAerolineaNick</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__18">            <from-activity-id>router</from-activity-id>            <control-flow-case id="__22">                <from-outcome>Listar</from-outcome>                <to-activity-id>getIdUsuarioNick</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__4">            <from-activity-id>ListaFrg</from-activity-id>            <control-flow-case id="__6">                <from-outcome>Editar</from-outcome>                <to-activity-id>ejecutarByIndice</to-activity-id>            </control-flow-case>            <control-flow-case id="__12">                <from-outcome>Create</from-outcome>                <to-activity-id>router2</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__13">            <from-activity-id>getIdAerolineaNick</from-activity-id>            <control-flow-case id="__14">                <from-outcome>getIdAerolineaNick</from-outcome>                <to-activity-id>getIdAeropuertoNick</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__15">            <from-activity-id>getIdAeropuertoNick</from-activity-id>            <control-flow-case id="__19">                <from-outcome>getIdAeropuertoNick</from-outcome>                <to-activity-id>ejecutarByIndiceReturn_Aerolinea</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__9">            <from-activity-id>ejecutarConsulta</from-activity-id>            <control-flow-case id="__10">                <from-outcome>ejecutarConsulta</from-outcome>                <to-activity-id>ListaFrg</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__21">            <from-activity-id>ejecutarByIndiceReturn_Aerolinea</from-activity-id>            <control-flow-case id="__24">                <from-outcome>ejecutarByIndiceReturn</from-outcome>                <to-activity-id>ejecutarByIndiceReturn_Aeropuerto</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__25">            <from-activity-id>ejecutarByIndiceReturn_Aeropuerto</from-activity-id>            <control-flow-case id="__26">                <from-outcome>ejecutarByIndiceReturn</from-outcome>                <to-activity-id>ActionCrear</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__28">            <from-activity-id>SessionBean_Limpar</from-activity-id>            <control-flow-case id="__29">                <from-outcome>limpiar</from-outcome>                <to-activity-id>ItemFrg</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__30">            <from-activity-id>Create</from-activity-id>            <control-flow-case id="__31">                <from-outcome>Create</from-outcome>                <to-activity-id>SessionBean_Limpar</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__16">            <from-activity-id>base_isUsuarioRol</from-activity-id>            <control-flow-case id="__17">                <from-outcome>base_isUsuarioRol</from-outcome>                <to-activity-id>ejecutarConsulta</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__32">            <from-activity-id>getIdUsuarioNick</from-activity-id>            <control-flow-case id="__33">                <from-outcome>getIdUsuarioNick</from-outcome>                <to-activity-id>base_isUsuarioRol</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__34">            <from-activity-id>router2</from-activity-id>            <control-flow-case id="__35">                <from-outcome>isCLI02</from-outcome>                <to-activity-id>router1</to-activity-id>            </control-flow-case>            <control-flow-case id="__36">                <from-outcome>noIsCLI02</from-outcome>                <to-activity-id>ActionCrear</to-activity-id>            </control-flow-case>        </control-flow-rule>        <control-flow-rule id="__2">            <from-activity-id>ejecutarByIndice</from-activity-id>            <control-flow-case id="__3">                <from-outcome>Editar</from-outcome>                <to-activity-id>ActionEditar</to-activity-id>            </control-flow-case>        </control-flow-rule>        <use-page-fragments/>    </task-flow-definition></adfc-config>