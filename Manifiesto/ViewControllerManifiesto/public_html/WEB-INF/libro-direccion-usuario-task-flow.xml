<?xml version="1.0" encoding="UTF-8" ?><adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">  <task-flow-definition id="libro-direccion-usuario-task-flow">    <template-reference>      <document>/WEB-INF/plantilla/detalle-template.xml</document>      <id>detalle-template</id>    </template-reference>    <managed-bean id="__1">      <managed-bean-name>PPRBackingErrorRest</managed-bean-name>      <managed-bean-class>viewManifiesto.errorRest.PPRListaFrg</managed-bean-class>      <managed-bean-scope>request</managed-bean-scope>    </managed-bean>    <managed-bean id="__2">      <managed-bean-name>PPRBackingPreliquidacionItemFrg</managed-bean-name>      <managed-bean-class>viewManifiesto.preliquidacion.PPRItemFrg</managed-bean-class>      <managed-bean-scope>request</managed-bean-scope>    </managed-bean>    <managed-bean id="__3">      <managed-bean-name>PPRBackingManifiestoUpListaFrg</managed-bean-name>      <managed-bean-class>viewManifiesto.up.PPRListaFrg</managed-bean-class>      <managed-bean-scope>backingBean</managed-bean-scope>    </managed-bean>    <method-call id="Recargar">      <method>#{bindings.setCurrentRowWithKeyValue.execute}</method>      <outcome>        <fixed-outcome>Recargar</fixed-outcome>      </outcome>    </method-call>    <method-call id="Create">      <method>#{bindings.CreateWithParams.execute}</method>      <outcome>        <fixed-outcome>Create</fixed-outcome>      </outcome>    </method-call>    <view id="ListaFrg">      <page>/libroDireccion/usuario/ListaFrg.jsff</page>    </view>    <view id="ItemFrg">      <page>/libroDireccion/usuario/ItemFrg.jsff</page>    </view>    <use-page-fragments/>  </task-flow-definition></adfc-config>