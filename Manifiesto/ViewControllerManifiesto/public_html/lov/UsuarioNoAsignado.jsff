<?xml version='1.0' encoding='UTF-8'?><jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"          xmlns:f="http://java.sun.com/jsf/core">    <af:panelGroupLayout id="pgl2">        <af:resource type="javascript">          //P1Usuario          function hidePopupP1Usuario() {              var popup = AdfPage.PAGE.findComponentByAbsoluteId('p1Usuario');              popup.hide();          }        </af:resource>        <af:panelGroupLayout id="pgl3" layout="horizontal">            <af:button id="b1" icon="../../Imagen-001/icons/7548857_search_glass_interface_user_magnifier_zoom_find.svg"                       disabled="#{!pageFlowScope.buscar}">                <af:showPopupBehavior popupId="p1Usuario"/>            </af:button>            <af:outputText value="#{bindings.Nick.inputValue} #{bindings.Nombre.inputValue}" id="ot1" truncateAt="45"                           rendered="#{(pageFlowScope.idValueInt ne null) or (pageFlowScope.idValueInt ge 0)}"/>            <af:outputText value="#{bnd.msg_no_definido}" id="ot4"                           rendered="#{(pageFlowScope.idValueInt eq null) or (pageFlowScope.idValueInt eq 0)}"/>            <f:facet name="separator">                <af:spacer id="s2" height="3" width="3"/>            </f:facet>        </af:panelGroupLayout>        <af:popup childCreation="deferred" id="p1Usuario">            <af:dialog id="d1" type="cancel" title="#{bnd.btn_buscar} Usuario">                <af:panelGroupLayout layout="vertical" id="pgl1">                    <af:query id="qryId1" value="#{bindings.UsuarioAsignadoViewNoDMLCriteriaQuery.queryDescriptor}"                              model="#{bindings.UsuarioAsignadoViewNoDMLCriteriaQuery.queryModel}"                              queryListener="#{bindings.UsuarioAsignadoViewNoDMLCriteriaQuery.processQuery}"                              queryOperationListener="#{bindings.UsuarioAsignadoViewNoDMLCriteriaQuery.processQueryOperation}"                              headerText="#{bnd.msg_busquea_titulo}" conjunctionReadOnly="true"                              searchButtonText="#{bnd.btn_buscar}" resetButtonText="#{bnd.btn_limpiar}" labelWidth="240"                              saveQueryMode="hidden" modeChangeVisible="false" disclosed="true"                              resultComponentId="::resId1"/>                    <af:table value="#{bindings.UsuarioAsignadoViewNoDML1.collectionModel}" var="row"                              rows="#{bindings.UsuarioAsignadoViewNoDML1.rangeSize}"                              emptyText="#{bindings.UsuarioAsignadoViewNoDML1.viewable ? 'No data to display.' : 'Access Denied.'}"                              selectedRowKeys="#{bindings.UsuarioAsignadoViewNoDML1.collectionModel.selectedRow}"                              selectionListener="#{bindings.UsuarioAsignadoViewNoDML1.collectionModel.makeCurrent}"                              rowSelection="single" fetchSize="#{bindings.UsuarioAsignadoViewNoDML1.rangeSize}"                              scrollPolicy="page" rowBandingInterval="1" id="resId1" columnStretching="column:resId1c6"                              inlineStyle="width:60vw;">                        <?audit suppress oracle.adf.faces.compreqsummary?><?audit suppress oracle.adf.faces.tableneedsrowheader?>                        <af:column headerText="#{bindings.UsuarioAsignadoViewNoDML1.hints.IdUsuario.label}"                                   id="resId1c1">                            <af:link text="#{row.IdUsuario}" id="l1" styleClass="linkSelected"                                     rendered="#{row.IdLibroDireccionAerolinea eq null}">                                <af:clientListener method="hidePopupP1Usuario" type="action"/>                                <!--<af:setActionListener from="#{row.IdUsuario}"                                                      to="#{BackingLOVFrg.idValueSeleccionado}"/>-->                            </af:link>                            <af:outputText value="#{row.IdUsuario}" rendered="#{row.IdLibroDireccionAerolinea ne null}"                                           id="ot7" inlineStyle="font-weight:bolder;"/>                        </af:column>                        <af:column headerText="#{bindings.UsuarioAsignadoViewNoDML1.hints.Nick.label}" id="resId1c2">                            <af:outputText value="#{row.Nick}"                                           shortDesc="#{bindings.UsuarioAsignadoViewNoDML1.hints.Nick.tooltip}"                                           id="ot2"/>                        </af:column>                        <af:column headerText="#{bindings.UsuarioAsignadoViewNoDML1.hints.Nombre.label}" id="resId1c3">                            <af:outputText value="#{row.Nombre}"                                           shortDesc="#{bindings.UsuarioAsignadoViewNoDML1.hints.Nombre.tooltip}"                                           id="ot3"/>                        </af:column>                        <af:column headerText="#{bindings.UsuarioAsignadoViewNoDML1.hints.Apellido.label}"                                   id="resId1c4">                            <af:outputText value="#{row.Apellido}"                                           shortDesc="#{bindings.UsuarioAsignadoViewNoDML1.hints.Apellido.tooltip}"                                           id="ot8"/>                        </af:column>                        <af:column headerText="#{bindings.UsuarioAsignadoViewNoDML1.hints.IdLibroDireccionAerolinea.label}"                                   id="resId1c5" rendered="false">                            <af:outputText value="#{row.IdLibroDireccionAerolinea}"                                           shortDesc="#{bindings.UsuarioAsignadoViewNoDML1.hints.IdLibroDireccionAerolinea.tooltip}"                                           id="ot5">                                <af:convertNumber groupingUsed="false"                                                  pattern="#{bindings.UsuarioAsignadoViewNoDML1.hints.IdLibroDireccionAerolinea.format}"/>                            </af:outputText>                        </af:column>                        <af:column headerText="#{bindings.UsuarioAsignadoViewNoDML1.hints.NombreAerolinea.label}"                                   id="resId1c6">                            <af:outputText value="#{row.NombreAerolinea}"                                           shortDesc="#{bindings.UsuarioAsignadoViewNoDML1.hints.NombreAerolinea.tooltip}"                                           id="ot6"/>                        </af:column>                    </af:table>                    <f:facet name="separator">                        <af:spacer id="s3" height="3" width="3"/>                    </f:facet>                </af:panelGroupLayout>            </af:dialog>        </af:popup>    </af:panelGroupLayout></jsp:root>